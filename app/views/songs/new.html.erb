<h1>Create Song</h1>

<%= form_for(@song) do |f| %>

<%= f.label :title %><br>
<%= f.text_field :title %><br>

<%= f.label :artist_id %><br>
<%= f.text_field :artist_id %><br>

<%= f.label :genre_id %><br>
<%= f.text_field :genre_id %><br>

<%= f.label :release_date %><br>
<%= f.date_field :release_date %><br>

<%= f.label :link %><br>
<%= f.text_field :link %><br><br>

<%= f.submit "Create Song" %>

<% end %>